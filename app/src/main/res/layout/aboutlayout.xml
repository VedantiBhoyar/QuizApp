<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/developer"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="408dp"
        android:layout_height="60dp"
        android:layout_marginBottom="506dp"
        android:background="@color/blue"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:title="QuizApp"
        app:titleTextColor="@color/white" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="236dp"
        android:layout_height="91dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="120dp"
        android:text="Developers"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="34sp"
        android:textStyle="bold|italic" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="71dp"
        android:layout_height="70dp"
        android:layout_marginStart="10dp"
        android:layout_below="@id/textView2"
        android:layout_marginTop="51dp"
        android:src="@drawable/v_symbol" />

    <TextView
        android:id="@+id/editTextText2"
        android:layout_width="221dp"
        android:layout_height="49dp"
        android:layout_below="@id/textView2"
        android:layout_marginStart="120dp"
        android:layout_marginTop="64dp"
        android:inputType="text"
        android:text="Vedanti Bhoyar"
        android:textColor="@color/white"
        android:textSize="24dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/editTextText3"
        android:layout_width="238dp"
        android:layout_height="67dp"
        android:layout_below="@id/editTextText2"
        android:layout_marginStart="110dp"
        android:layout_marginTop="15dp"
        android:inputType="text"
        android:text="bhoyar.veda@gmail.com"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="72dp"
        android:layout_height="79dp"
        android:layout_below="@id/imageView"
        android:layout_marginStart="10dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="42dp"
        android:src="@drawable/s_symbol" />

    <TextView
        android:id="@+id/editTextText4"
        android:layout_width="276dp"
        android:layout_height="57dp"
        android:layout_below="@id/imageView"
        android:layout_marginStart="120dp"
        android:layout_marginTop="99dp"
        android:inputType="text"
        android:text="Swaraj Shelavale"
        android:textColor="@color/white"
        android:textSize="24dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/editTextText5"
        android:layout_width="295dp"
        android:layout_height="74dp"
        android:layout_below="@id/editTextText4"
        android:layout_marginStart="100dp"
        android:layout_marginTop="5dp"
        android:inputType="text"
        android:text="shelavaleswaraj16@gmail.com"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button3"
        android:layout_width="358dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageView2"
        android:layout_marginStart="39dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="116dp"
        android:backgroundTint="@color/blue"
        android:text="Back"
        android:textColor="@color/white" />


</RelativeLayout>